class Solution:
    def hammingWeight(self, n: int) -> int:
        counter= 0
        counter2 = 0
        one_str = str(n)
        while counter2 < len(one_str):
            if one_str[counter2]=="1":
                counter+=1
            counter2+=1
        return counter
 #does not give the correct output for complicated testcases, though
